{
  "name": "ecommerce",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env VUE_APP_BUILDING=false vue-cli-service build && yarn remove-localhost",
    "build-data": "node scripts/build-data.js",
    "lint": "vue-cli-service lint",
    "deploy": "yarn get-data && yarn build && now",
    "release": "yarn lint && release-it && yarn deploy",
    "postversion": "node scripts/postversion.js",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "get-data": "yarn build-data && yarn get-shopify-data && yarn create-sitemap",
    "get-shopify-data": "node ./scripts/get-shopify-data.js",
    "remove-localhost": "node ./scripts/remove-localhost-dist.js",
    "create-sitemap": "node ./scripts/create-sitemap.js"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "firebase": "^7.6.2",
    "pageclip": "^1.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuetify": "^2.1.11",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/js": "*",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "archiver": "^3.1.1",
    "axios": "*",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cli-spinner": "^0.2.10",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-vuetify": "*",
    "eslint-plugin-vue": "^5.0.0",
    "html-minifier": "^4.0.0",
    "node-fetch": "^3.1.1",
    "prerender-spa-plugin": "^3.4.0",
    "recursive-readdir": "^2.2.2",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "shell": "^0.5.0",
    "shelljs": "^0.8.3",
    "shopify-buy": "^2.7.1",
    "vue-analytics": "*",
    "vue-cli-plugin-eslint-config-vuetify": "latest",
    "vue-cli-plugin-vuetify": "latest",
    "vue-cli-plugin-vuetify-essentials": "latest",
    "vue-cli-plugin-webpack-bundle-analyzer": "^1.4.0",
    "vue-meta": "*",
    "vue-scrollactive": "^0.8.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "vuex-pathify": "*",
    "vuex-router-sync": "*",
    "webfontloader": "*"
  }
}
